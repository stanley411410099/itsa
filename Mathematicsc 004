#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <ctype.h>

int main(){
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d+%d=%d\n",a,b,a+b);
    printf("%d*%d=%d\n",a,b,a*b);
    printf("%d-%d=%d\n",a,b,a-b);
    if(a%b<0){
        printf("%d/%d=%d...%d\n",a,b,a/b-1,a%b+b);
        return 0;
    }
    printf("%d/%d=%d...%d\n",a,b,a/b,a%b);
}
