#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <ctype.h>

int main(){
    float a,b;
    scanf("%f %f",&a,&b);
    if(a<=60){
        printf("%.1f\n",a*b);
    } 
    else if(60<a && a<121){
        printf("%.1f\n",60*b+(a-60)*b*1.33);
    }
    else{
        printf("%.1f\n",60*b+60*b*1.33+(a-120)*b*1.66);
    }
    
}
