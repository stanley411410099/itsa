#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <ctype.h>

int main(){
    int n,n2,total=0;
    scanf("%d %d",&n,&n2);
    if(n>n2){
        for(int i=n2;i<=n;i++){
            total=total+i;
        }
        printf("%d\n",total);
        return 0;
    }
    for(int i=n;i<=n2;i++){
            total=total+i;
        }
    printf("%d\n",total);
}
